{
  "name": "swenson-he",
  "version": "0.0.9",
  "private": true,
  "npmClient": "yarn",
  "engines": {
    "node": ">=16.20.0",
    "yarn": ">=1.22.19",
    "npm": "please-use-yarn"
  },
  "scripts": {
    "dev": "cross-env NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc -p tsconfig.json --noEmit",
    "lint": "next lint --max-warnings=0 . --ext=.ts,.tsx",
    "lint:fix": "yarn lint --fix",
    "tsprune": "yarn ts-prune -e -i '(assets|server|public|src/index.ts|components/src/index.ts)'",
    "tsprune:all": "yarn ts-prune -e -i",
    "pre-commit": "lint-staged",
    "pre-push": "git diff --quiet HEAD origin/main -- src || yarn build",
    "prepare": "husky install"
  },
  "dependencies": {
    "next": "^13.4.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "^5.0.4",
    "swr": "^2.1.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@eslint-recommended/eslint-config": "^10.0.0",
    "@next/eslint-plugin-next": "^13.4.2",
    "@typescript-eslint/types": "^5.59.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/utils": "^5.48.1",
    "@typescript-eslint/parser": "^5.59.8",
    "@swensonhe/eslint-plugin": "file:eslint-plugin/lib",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "prettier": "^2.8.8",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "^13.4.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-import-resolver-babel-module": "^5.3.2",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^8.0.3",
    "lint-staged": "^10.5.3",
    "ts-prune": "^0.10.3",
    "cross-env": "^7.0.3"
  },
  "resolutions": {
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint -- --quiet --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
