{
  "name": "swenson-he",
  "version": "0.0.9",
  "private": true,
  "npmClient": "yarn",
  "engines": {
    "node": ">=16.20.0",
    "yarn": ">=1.22.19",
    "npm": "please-use-yarn"
  },
  "scripts": {
    "dev": "cross-env NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint --max-warnings=0 . --ext=.ts,.tsx",
    "lint:fix": "eslint --fix",
    "tsprune": "yarn ts-prune -e -i '(assets|server|public|src/index.ts|components/src/index.ts)'",
    "tsprune:all": "yarn ts-prune -e -i",
    "format": "prettier \"**/*.+(json|ts|tsx)\"",
    "format:check": "npm run prettier -- --check",
    "format:fix": "npm run prettier -- --write",
    "pre-commit": "lint-staged",
    "pre-push": "git diff --quiet HEAD origin/main -- src || yarn build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.0",
    "classnames": "^2.3.2",
    "framer-motion": "^10.16.1",
    "next": "^13.4.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-no-ssr": "^1.1.0",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2",
    "swr": "^2.2.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.x",
    "@babel/eslint-parser": "^7.22.11",
    "@babel/plugin-syntax-jsx": "7.x",
    "@babel/preset-react": "7.x",
    "@commitlint/cli": "^17.2.1",
    "@commitlint/config-conventional": "^17.2.1",
    "@svgr/webpack": "^8.1.0",
    "@swensonhe/eslint-plugin": "file:eslint-plugin/lib",
    "@types/node": "20.5.6",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-no-ssr": "^1.1.3",
    "@types/uuid": "^9.0.3",
    "@typescript-eslint/eslint-plugin": "5.6.0",
    "@typescript-eslint/parser": "5.6.0",
    "@typescript-eslint/utils": "^6.5.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.48.0",
    "eslint-config-airbnb": "18.x",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-next": "^13.4.19",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-babel-module": "^5.3.2",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "7.x",
    "eslint-plugin-react-hooks": "4.x",
    "husky": "^8.0.3",
    "lint-staged": "^10.5.3",
    "prettier": "^3.0.2",
    "ts-prune": "^0.10.3",
    "typescript": "^4.7.2"
  },
  "resolutions": {
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.0",
    "**/eslint": "7.11.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn eslint --max-warnings=0 . --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
