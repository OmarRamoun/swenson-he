{
  "name": "swenson-he",
  "version": "0.0.9",
  "private": true,
  "npmClient": "yarn",
  "engines": {
    "node": ">=16.20.0",
    "yarn": ">=1.22.19",
    "npm": "please-use-yarn"
  },
  "scripts": {
    "dev": "cross-env NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc -p tsconfig.json --noEmit",
    "lint": "next lint --max-warnings=0 . --ext=.ts,.tsx",
    "lint:fix": "yarn lint --fix",
    "tsprune": "yarn ts-prune -e -i '(assets|server|public|src/index.ts|components/src/index.ts)'",
    "tsprune:all": "yarn ts-prune -e -i",
    "format": "prettier \"**/*.+(json|ts|tsx)\"",
    "format:check": "npm run prettier -- --check",
    "format:fix": "npm run prettier -- --write",
    "pre-commit": "lint-staged",
    "pre-push": "git diff --quiet HEAD origin/main -- src || yarn build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.0",
    "classnames": "^2.3.2",
    "framer-motion": "^10.16.1",
    "next": "^13.4.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2",
    "swr": "^2.2.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.2.1",
    "@commitlint/config-conventional": "^17.2.1",
    "@eslint-recommended/eslint-config": "^10.0.0",
    "@next/eslint-plugin-next": "^13.4.2",
    "@svgr/webpack": "^8.1.0",
    "@swensonhe/eslint-plugin": "file:eslint-plugin/lib",
    "@types/node": "20.5.6",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "@typescript-eslint/types": "^5.59.5",
    "@typescript-eslint/utils": "^5.48.1",
    "@babel/core": "^7.20.5",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-syntax-jsx": "^7.18.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "babel-eslint": "^10.1.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.30.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-config-next": "^13.4.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^10.5.3",
    "prettier": "^2.8.1",
    "prettier-eslint": "^15.0.1",
    "ts-prune": "^0.10.3",
    "typescript": "~4.5.2"
  },
  "resolutions": {
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.0",
    "**/eslint": "7.11.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint -- --quiet --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
